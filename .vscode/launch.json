  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug plugin",
      "type": "node",
      "request": "attach",
      "port": 54546,
      "restart": true,
      "outFiles": ["${workspaceFolder}/bin/**/*.js"],
      "resolveSourceMapLocations": ["${workspaceFolder}/**"],
      "preLaunchTask": "npm: watch"
    },
    {
      "name": "Debug property inspectors",
      "request": "launch",
      "type": "msedge",
      "url": "http://localhost:23654",
      "webRoot": "${workspaceFolder}"
    }
  ]
}
